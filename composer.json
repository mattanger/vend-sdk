{
    "name": "simplesquid/vend-sdk",
    "description": "An unoffical PHP SDK for Vend POS",
    "keywords": [
        "simplesquid",
        "vend-sdk",
        "sdk",
        "point-of-sale",
        "vend"
    ],
    "homepage": "https://github.com/simplesquid/vend-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Matthew Pouter",
            "email": "matthew.poulter@simplesquid.co.za",
            "homepage": "https://simplesquid.co.za/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.2",
        "spatie/data-transfer-object": "~3.9.1",
        "nesbot/carbon": "^2.72.2|^3.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "symfony/var-dumper": "^4.3",
        "phpunit/phpunit": "^8.2",
        "filp/whoops": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "SimpleSquid\\Vend\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimpleSquid\\Vend\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
